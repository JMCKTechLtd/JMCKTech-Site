{
  "routes": [
    {
      "route": "/courses/firstline/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  }
}
