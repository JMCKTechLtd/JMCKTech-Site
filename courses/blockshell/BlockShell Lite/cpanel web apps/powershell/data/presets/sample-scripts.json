{
  "version": "1.0.0",
  "description": "Sample Active Directory scripts for BlockShell Lite",
  "presets": [
    {
      "id": "ad-user-provisioning",
      "name": "AD User Provisioning",
      "description": "Creates a new AD user with basic setup",
      "blocks": [
        {
          "type": "create_ad_user",
          "fields": {
            "USER": "new.user"
          }
        },
        {
          "type": "set_user_properties",
          "fields": {
            "USER": "new.user",
            "EMAIL": "new.user@company.com",
            "DEPARTMENT": "IT"
          }
        },
        {
          "type": "set_user_password",
          "fields": {
            "USER": "new.user",
            "PASSWORD": "TempP@ss123!"
          }
        },
        {
          "type": "force_password_change",
          "fields": {
            "USER": "new.user"
          }
        },
        {
          "type": "add_user_to_group",
          "fields": {
            "USER": "new.user",
            "GROUP": "Domain Users"
          }
        }
      ]
    },
    {
      "id": "ad-security-audit",
      "name": "Security Audit",
      "description": "Basic security audit script",
      "blocks": [
        {
          "type": "check_locked_accounts",
          "fields": {
            "OU": "Users"
          }
        },
        {
          "type": "check_password_expiry",
          "fields": {
            "USER": "admin.user",
            "DAYS": 7
          }
        },
        {
          "type": "export_group_membership",
          "fields": {
            "USER": "admin.user"
          }
        }
      ]
    },
    {
      "id": "ad-group-management",
      "name": "Group Management",
      "description": "Manage AD groups and members",
      "blocks": [
        {
          "type": "create_ad_group",
          "fields": {
            "GROUP": "NewProjectTeam"
          }
        },
        {
          "type": "add_user_to_group",
          "fields": {
            "USER": "user1",
            "GROUP": "NewProjectTeam"
          }
        },
        {
          "type": "add_user_to_group",
          "fields": {
            "USER": "user2",
            "GROUP": "NewProjectTeam"
          }
        },
        {
          "type": "get_group_members",
          "fields": {
            "GROUP": "NewProjectTeam",
            "EXPORT": false
          }
        }
      ]
    }
  ]
}